ers":[1,"ショタ1",0,0,30,35,80,80,0,0]},{"code":232,"indent":0,"parameters":[1,0,0,0,50,35,80,80,255,0,20,false]},{"code":234,"indent":0,"parameters":[3,[0,0,0,0],1,false]},{"code":234,"indent":0,"parameters":[1,[-68,-68,-68,0],1,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":[" ◆Shota"]},{"code":401,"indent":0,"parameters":["「...Aww.」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":[" ◆Shota"]},{"code":401,"indent":0,"parameters":["「Even though it's holidays from tomorrow"]},{"code":401,"indent":0,"parameters":["  we still have to take tests on the weekend.」"]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":[" ◆Shota"]},{"code":401,"indent":0,"parameters":["「Despite the fact that we'll have to take exams"]},{"code":401,"indent":0,"parameters":["  soon, did the school really had to do this?」"]},{"code":234,"indent":0,"parameters":[3,[-68,-68,-68,0],1,false]},{"code":234,"indent":0,"parameters":[1,[0,0,0,0],1,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":[" ◆\\N[1]"]},{"code":401,"indent":0,"parameters":["「...I-It can't be helped..."]},{"code":401,"indent":0,"parameters":["　The school is thinking about us too...」"]},{"code":231,"indent":0,"parameters":[1,"ショタ2",0,0,30,35,80,80,0,0]},{"code":232,"indent":0,"parameters":[1,0,0,0,50,35,80,80,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":[" ◆\\N[1]"]},{"code":401,"indent":0,"parameters":["「My grades aren't that good...」"]},{"code":250,"indent":0,"parameters":[{"name":"Earth5","volume":90,"pitch":150,"pan":0}]},{"code":225,"indent":0,"parameters":[5,5,20,false]},{"code":231,"indent":0,"parameters":[1,"ショタ3",0,0,30,35,80,80,0,0]},{"code":232,"indent":0,"parameters":[1,0,0,0,50,35,80,80,255,0,20,false]},{"code":101,"indent":0,"parameters":["",0,2,2]},{"code":401,"indent":0,"parameters":[" ◆\\N[1]"]},{"code":4